        <Sequence DisplayName="Assigning mail to transaction itemSequence" sap2010:WorkflowViewState.IdRef="Sequence_4">
          <Sequence.Variables>
            <Variable x:TypeArguments="scg:List(snm:MailMessage)" Name="Processing_Email" />
          </Sequence.Variables>
          <Assign DisplayName="Assign - Out transaction item" sap2010:WorkflowViewState.IdRef="Assign_5">
            <Assign.To>
              <OutArgument x:TypeArguments="snm:MailMessage">[out_TransactionItem]</OutArgument>
            </Assign.To>
            <Assign.Value>
              <InArgument x:TypeArguments="snm:MailMessage">[in_All_Mails(in_TransactionNumber-1)]</InArgument>
            </Assign.Value>
          </Assign>
          <ui:LogMessage DisplayName="Log Message" sap2010:WorkflowViewState.IdRef="LogMessage_2" Level="Info" Message="[&quot;Processing Mail Number : &quot;+out_TransactionItem.ToString]" />
          <ui:MoveOutlookMessage Account="[in_Config(&quot;Account_Name&quot;).ToString]" DisplayName="Move Outlook Mail Message" sap2010:WorkflowViewState.IdRef="MoveOutlookMessage_2" MailFolder="[in_Config(&quot;Processing_Email_Folder&quot;).ToString]" MailMessage="[out_TransactionItem]" />
          <Delay DisplayName="Delay to move mail" Duration="00:00:02" sap2010:WorkflowViewState.IdRef="Delay_1" />
          <ui:GetOutlookMailMessages Filter="{x:Null}" TimeoutMS="{x:Null}" Account="[in_Config(&quot;Account_Name&quot;).ToString]" DisplayName="Get Outlook Mail Messages from processing folder" GetAttachements="False" sap2010:WorkflowViewState.IdRef="GetOutlookMailMessages_2" MailFolder="[in_Config(&quot;Processing_Email_Folder&quot;).ToString]" MarkAsRead="True" Messages="[Processing_Email]" OnlyUnreadMessages="True" Top="1" />
          <If Condition="[Processing_Email.Count=0]" DisplayName="If processing count is 0" sap2010:WorkflowViewState.IdRef="If_3">
            <If.Then>
              <Sequence DisplayName="Sequence to assign mail not found" sap2010:WorkflowViewState.IdRef="Sequence_5">
                <Assign DisplayName="Assign - Mail not found" sap2010:WorkflowViewState.IdRef="Assign_8">
                  <Assign.To>
                    <OutArgument x:TypeArguments="ui:GenericValue">[io_mail_Item_not_Found]</OutArgument>
                  </Assign.To>
                  <Assign.Value>
                    <InArgument x:TypeArguments="ui:GenericValue">[1]</InArgument>
                  </Assign.Value>
                </Assign>
                <ui:LogMessage DisplayName="Log Message" sap2010:WorkflowViewState.IdRef="LogMessage_6" Level="Info" Message="[&quot;Mail not found in processing folder. Moving to next Email&quot;]" />
              </Sequence>
            </If.Then>
            <If.Else>
              <Assign DisplayName="Assign - Out transcation item" sap2010:WorkflowViewState.IdRef="Assign_7">
                <Assign.To>
                  <OutArgument x:TypeArguments="snm:MailMessage">[out_TransactionItem]</OutArgument>
                </Assign.To>
                <Assign.Value>
                  <InArgument x:TypeArguments="snm:MailMessage">[Processing_Email(0)]</InArgument>
                </Assign.Value>
              </Assign>
            </If.Else>
          </If>
        </Sequence>
      </If.Else>
    </If>
